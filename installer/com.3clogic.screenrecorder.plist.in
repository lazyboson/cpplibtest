<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
<key>Label</key>
<string>@BUNDLE_ID@</string>
<key>ProgramArguments</key>
<array>
<string>/Applications/@BUNDLE_NAME@.app/Contents/MacOS/@BUNDLE_NAME@</string>
</array>
<key>RunAtLoad</key>
<true/>
<key>KeepAlive</key>
<dict>
<key>SuccessfulExit</key>
<false/>
<key>Crashed</key>
<true/>
</dict>
<key>ProcessType</key>
<string>Interactive</string>
<key>ThrottleInterval</key>
<integer>10</integer>
<key>StandardOutPath</key>
<string>/tmp/3clogic_screenrecorder.log</string>
<key>StandardErrorPath</key>
<string>/tmp/3clogic_screenrecorder_error.log</string>
<key>WorkingDirectory</key>
<string>/Applications/@BUNDLE_NAME@.app</string>
<key>EnvironmentVariables</key>
<dict>
<key>PATH</key>
<string>/usr/local/bin:/usr/bin:/bin:/usr/sbin:/sbin</string>
</dict>
</dict>
</plist>